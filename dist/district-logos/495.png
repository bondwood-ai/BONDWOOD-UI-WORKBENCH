            <script>
                $(document).ready(function(){
                    
                // For screen reader
                // $('ul.navbar-nav').attr('role','menu'); bypass related 103571
                $('ul.navbar-nav > li > a').attr({
                    role:'menuitem'
                });

                // press enter key to open dropdown menu
                var toggled = false;
                $('ul.navbar-nav > li > a').keydown(function(e) {
                    if(e.keyCode == 13 && $(this).siblings('.dropdown-menu').hasClass('sub-menu') !== false ) { // enter key
                        toggled = !toggled;
                        e.preventDefault();
                        $('ul.navbar-nav > li').not($(this).parent()).removeClass('force-show');
                        $('ul.navbar-nav > li > a').siblings('.dropdown-menu').not($(this).siblings('.dropdown-menu')).css({
                                                                                            display: 'none',
                                                                                            visibility: 'hidden',
                                                                                            opacity: '0'
                                                                                        });
                        // $(this).parent().attr('class', toggled ? 'force-show':'');
                        $(this).parent().addClass('force-show');
                        $(this).siblings('.dropdown-menu').css({
                                                        display: 'block',
                                                        visibility: 'visible',
                                                        opacity: '1'
                                                        });
                        $(this).attr('aria-expanded', toggled ? 'true':'false');
                    }
                    if(e.keyCode == 40) { // down arrow, focus to first link
                        e.preventDefault();
                        e.stopPropagation();
                        $(this).siblings('.dropdown-menu').find('a').first().focus();
                    }
                });

                // For menu level 1
                $('ul.navbar-nav > li.dropdown > ul.dropdown-menu > li.dropdown > a').keydown(function(e){
                    if($(this).attr('href') === '#' || $(this).attr('href') === 'javascript:void(0);' || $(this).siblings('.dropdown-menu').hasClass('sub-menu') !== false){

                        if(e.keyCode == 13 ){ // enter key
                            e.preventDefault();
                            e.stopPropagation();
                            $('ul.navbar-nav > li.dropdown > ul.dropdown-menu > li.dropdown').removeClass('force-show');
                            $('ul.navbar-nav > li.dropdown > ul.dropdown-menu > li.dropdown').find('.dropdown-menu').not($(this).siblings('.dropdown-menu')).css({
                                                                                            display: 'none',
                                                                                            visibility: 'hidden',
                                                                                            opacity: '0'
                                                                                        });

                            $(this).parent().addClass('force-show');
                            $(this).siblings('.dropdown-menu').css({
                                                        display: 'block',
                                                        visibility: 'visible',
                                                        opacity: '1'
                                                        });
                        }
                        if(e.keyCode == 39) { // right arrow, focus to first link
                            e.preventDefault();
                            e.stopPropagation();
                            $(this).siblings('.dropdown-menu').find('a').first().focus();
                        }
                    }

                    if(e.keyCode == 37) { // left arrow, collapse submenu
                        e.preventDefault();
                        e.stopPropagation();
                        $(this).parent().parent().prev().prev().focus();
                        $(this).parent().parent().css({
                                                    display: 'none',
                                                    visibility: 'hidden',
                                                    opacity: '0'
                                                });
                    }
                });

                // For menu level > 1
                $('ul.dropdown-menu li.dropdown a').keydown(function(e){
                    if($(this).attr('href') === '#' || $(this).attr('href') === 'javascript:void(0);' || $(this).siblings('.dropdown-menu').hasClass('sub-menu') !== false){
                        if(e.keyCode == 13){ // enter key
                            e.preventDefault();
                            e.stopPropagation();
                            $('ul.dropdown-menu li.dropdown').removeClass('force-show');
                            $(this).parent().addClass('force-show');
                            $(this).siblings('.dropdown-menu').css({
                                                        display: 'block',
                                                        visibility: 'visible',
                                                        opacity: '1'
                                                        });
                        }
                        if(e.keyCode == 39) { // right arrow, focus to first link
                            e.preventDefault();
                            e.stopPropagation();
                            $(this).siblings('.dropdown-menu').find('a').first().focus();
                        }
                    }

                    if(e.keyCode == 37) { // left arrow, collapse submenu
                        e.preventDefault();
                        e.stopPropagation();
                        $(this).parent().parent().prev().prev().focus();
                        $(this).parent().parent().css({
                                                    display: 'none',
                                                    visibility: 'hidden',
                                                    opacity: '0'
                                                });
                    }
                });

                // Bug 101179
                var desktopView = window.matchMedia('(min-width: 768px)');
                if(desktopView.matches){
                    $('body').click(function(){
                        $('ul.navbar-nav').find('.dropdown-menu').css({
                                                       display: 'none',
                                                       visibility: 'hidden',
                                                       opacity: '0'
                        })
                    });
                }

                // Bug 84403
                var mediaQuery = window.matchMedia('(max-width: 767px)');
                if(mediaQuery.matches){
                    $('ul.navbar-nav > li.dropdown > a').attr('href','#');
                    $('ul.navbar-nav li.dropdown a[style~="underline"]').removeAttr('data-toggle', 'dropdown');
                }
            
                $('.dropdown-menu > li.dropdown > a').mouseover(function() {
                    var el = $(this).next('.dropdown-menu');
                    var offset = el.offset();

                    var val = parseInt($(this).outerWidth()) + parseInt($(this).position().left);

                    if(parseInt($(this).offset().left) + parseInt($(this).outerWidth()) + parseInt(el.outerWidth()) > $(window).width() || parseInt(el.css('left')) < 0 ) {
                        el.css({
                            right: val,
                            left: 'auto'
                        });
                    } else {
                        el.css({
                            right: 'auto',
                            left: val
                        });
                    }

                });
            
                    // access search input
                    $('#search-form').keypress(function(e){
                        if(e.keyCode == 13){ // enter key
                            e.preventDefault();
                            $(this).addClass('openclick');
                        }
                    });
                    // add href attribute to all nav menu
                    $('ul.navbar-nav a:not([href])').attr('href','#');
                    // press shift+tab key focus to previous link
                    $('body').keypress(function (e){
                        if(e.shiftKey && e.keyCode == 9) {
                            e.preventDefault();
                            e.stopPropagation();
                            $(this).closest('li').prev().find('a').focus();
                        }
                    })
                    // press enter key to open dropdown menu
                    var toggled = false;
                    $('ul.navbar-nav > li > a').keydown(function(e) {
                        if(e.keyCode == 13 && $(this).next().hasClass('sub-menu') !== false ) { // enter key
                            toggled = !toggled;
                            e.preventDefault();
                            $('ul.navbar-nav > li').not($(this).parent()).removeClass('force-show');
                            // $(this).parent().attr('class', toggled ? 'force-show':'');
                            $(this).parent().addClass('force-show');
                            $(this).attr('aria-expanded', toggled ? 'true':'false');
                        }
                        if(e.keyCode == 40) { // down arrow, focus to first link
                            e.preventDefault();
                            e.stopPropagation();
                            $(this).next().find('a').first().focus();
                        }
                    });
                    // press up/down & end/home key to access submenu
                    $('.dropdown-menu > li > a').keydown(function(e){
                        if(e.keyCode == 40) { // down arrow
                            e.preventDefault();
                            e.stopPropagation();
                            $(this).closest('li').next().find('a').focus();
                        }
                        if(e.keyCode == 38) { // up arrow
                            e.preventDefault();
                            e.stopPropagation();
                            $(this).closest('li').prev().find('a').focus();
                        }
                        if(e.keyCode == 36) { // home key
                            e.preventDefault();
                            $(this).closest('.dropdown-menu').find('a:first').focus();
                        }
                        if(e.keyCode == 35) { // end key
                            e.preventDefault();
                            $(this).closest('.dropdown-menu').find('a:last').focus();
                        }
                        // For sub menu level 2
                        if($(this).attr('href') === '#'){
                            if(e.keyCode == 13){ // enter key
                                e.preventDefault();
                                e.stopPropagation();
                                $('.dropdown-menu > li').removeClass('force-show');
                                $(this).parent().addClass('force-show');
                            }
                            if(e.keyCode == 40) { // down arrow, focus to first link
                                e.preventDefault();
                                e.stopPropagation();
                                $(this).next().find('a').first().focus();
                            }
                        }
                    })
                    $('body').click(function () {
                        $('*').removeClass('force-show');

                    });
                });
            </script>
        

                <script type='text/javascript'>
                    $(document).ready(function(){
                        var skipId = $('div.block-hs section').first().attr('id');;

                        if(skipId==undefined){
                            skipId = $('.bottom2-header').attr('id');
                        }else{
                            skipId = skipId; 
                        }

                        $('body').prepend('<div class="skip-content"><a href="#'+skipId+'" class="skip-nav"><strong>Skip to main content</strong></a></div>');

                        $('a.skip-nav').click(function(event) {
                            var height = parseInt($('.sticky-hs').outerHeight());
                            if(isNaN(height) || height === null){
                                height = 0;
                            }else{
                                height = height;
                            }
                            if (this.hash !== '') {
                                event.preventDefault();
                                var hash = this.hash;
                                $('html, body').animate({
                                  scrollTop: $(hash).offset().top - height
                                }, 500, function(){
                                  window.location.hash = hash;
                                });
                                console.log(height);
                            }

                        });
                    });
                </script>
            
