                    $(window).load(function(){

                        var waitForEl = function(selector, callback) {
                          if ($(selector).length) {
                            callback();
                          } else {
                            setTimeout(function() {
                              waitForEl(selector, callback);
                            }, 100);
                          }
                        };

                        waitForEl(".owl-nav", function() {
                            $(".owl-next, .owl-prev").append("<p style=display:none>Arrow</p>");
                        });

                        waitForEl(".owl-dots", function() {
