            <script>
                $(document).ready(function(){
                    $(".fa-caret-down").click(function(e){
                        e.preventDefault(); 
                        e.stopPropagation();

                        var dataToggle = $(this).prev().attr("data-toggle");

                        if (typeof dataToggle !== typeof undefined && dataToggle !== false) {
                            $(this).closest("li.dropdown").addClass("open");
                            $(this).prev().removeAttr("data-toggle");
                            $(this).prev().attr("aria-expanded","true");
                        }else{
                            $(this).closest("li.dropdown").removeClass("open");
                            $(this).prev().attr("data-toggle", "dropdown");
                            $(this).prev().attr("aria-expanded","false");
                        }
                        $(this).next().css("margin-left","10px");

                        var thisParent = $(this).parent();
                        $(this).parent().siblings().not(thisParent).removeClass("open");
                    });

                    $("ul.dropdown-menu li a").click(function(){
                        $(this).siblings(".dropdown-menu").css("margin-left","10px");
                    })
                });
            </script>

